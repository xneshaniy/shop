{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true"
    },
    "outputDirectory": "dist",
    "buildCommand": "npm run build && composer install --no-dev --optimize-autoloader && mkdir -p dist && cp -r public/* dist/"
}